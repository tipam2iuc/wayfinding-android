<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android = "http://schemas.android.com/apk/res/android"
             xmlns:tools = "http://schemas.android.com/tools"
             android:layout_width = "match_parent"
             android:layout_height = "match_parent"
             tools:context = ".Fragment.ConcourFragment" android:background = "#00C0BFBF">
	<android.support.v7.widget.RecyclerView android:layout_width = "match_parent"
	          android:id="@+id/list_concour_fragment"
	          android:layout_height = "match_parent"
	          android:clipChildren = "false"
	          android:clipToPadding = "false"
	          android:divider = "@android:color/transparent"
	          android:dividerHeight = "10dp"
	          android:paddingBottom = "@dimen/concour_vertical_margin"
	          android:paddingStart = "@dimen/concour_vertical_margin"
	          android:paddingLeft = "@dimen/concour_vertical_margin"
	          android:paddingRight = "@dimen/concour_vertical_margin"
	          android:paddingTop = "@dimen/concour_vertical_margin"
	          android:scrollbars = "vertical"
	          tools:listitem = "@layout/cell"
	>
	
	</android.support.v7.widget.RecyclerView>

</FrameLayout>