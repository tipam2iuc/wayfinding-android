<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android = "http://schemas.android.com/apk/res/android"
             xmlns:tools = "http://schemas.android.com/tools"
             android:layout_width = "match_parent"
             android:layout_height = "match_parent"
             tools:context = ".Fragment.SchoolFragment" android:background = "#CDFAF7FF">
	<android.support.v7.widget.RecyclerView
			android:layout_width = "match_parent"
			android:layout_height = "match_parent"
			android:id = "@+id/recycler_school_fragement"
			android:scrollbars = "vertical"
			
			android:clipChildren = "false"
			android:clipToPadding = "false"
			android:divider = "@android:color/transparent"
			android:dividerHeight = "10dp"
			android:paddingBottom = "@dimen/concour_vertical_margin"
			android:paddingStart = "@dimen/concour_vertical_margin"
			android:paddingLeft = "@dimen/concour_vertical_margin"
			android:paddingRight = "@dimen/concour_vertical_margin"
			android:paddingTop = "@dimen/concour_vertical_margin"
			tools:listitem = "@layout/cell_school"
	
	/>
</FrameLayout>